{
    "name": "@etsoo/restclient",
    "version": "1.0.2",
    "description": "TypeScript REST client supports Axios and Fetch",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "build": "tsc",
        "lint": "eslint --ext .ts,.tsx src/",
        "test": "jest",
        "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand"
    },
    "jest": {
        "automock": false,
        "testMatch": [
            "<rootDir>/tests/**/*.ts"
        ],
        "testEnvironment": "node",
        "transform": {
            ".+\\.ts$": "ts-jest"
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ETSOO/restclient.git"
    },
    "keywords": [],
    "author": "Garry Xiao",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ETSOO/restclient/issues"
    },
    "homepage": "https://github.com/ETSOO/restclient#readme",
    "dependencies": {
        "axios": "^0.19.2",
        "typescript": "^3.9.7",
        "ts-jest": "^26.1.4"
    },
    "devDependencies": {
        "@types/jest": "^26.0.8",
        "@typescript-eslint/eslint-plugin": "^3.7.1",
        "@typescript-eslint/parser": "^3.7.1",
        "eslint": "^7.6.0",
        "eslint-config-airbnb-base": "^14.2.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.22.0",
        "jest": "^26.2.2",
        "jest-fetch-mock": "^3.0.3",
        "prettier": "^2.0.5"
    }
}
